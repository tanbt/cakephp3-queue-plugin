{
    "name": "trungtan/cakephp3-queue-plugin",
    "description": "CakeQueue plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "seo", "plugin", "queue"],
    "homepage": "https://bitbucket.org/trungtan/cakephp3-queue-plugin",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Tan Bui",
            "email": "trungtanbui@gmail.com",
            "role": "Author"
        },
        {
            "name": "Do Quoc Bao",
            "email": "rearmlkp@gmail.com",
            "role": "Contributor"
        }
    ],

    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "aws/aws-sdk-php": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeQueue\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
